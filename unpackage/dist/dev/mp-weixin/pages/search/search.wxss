
.history-container {
  margin-top: 16rpx;
  padding: 0 24rpx;
}
.history-title {
  margin-bottom: 16rpx;
  font-size: 30rpx;
}
.history {
  display: flex;
  flex-wrap: wrap;
  font-size: 26rpx;
  margin-right: -24rpx;
}
.history-item {
  flex-shrink: 0;
  margin: 0 24rpx 24rpx 0;
  padding: 4rpx 8rpx;
  border: 1px solid #ccc;
  border-radius: 8rpx;
}
.history-clear {
  margin-top: 24rpx;
  text-align: center;
  font-size: 26rpx;
  color: #666;
}
